{
  "name": "micro-pr-triage",
  "description": "Triage PRs from GitHub webhooks",
  "version": "1.5.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "github"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-pr-triage.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-pr-triage/issues"
  },
  "homepage": "https://github.com/telemark/micro-pr-triage#readme",
  "now": {
    "alias": [
      "triage.pr.github.tjeneste.win"
    ],
    "type": "npm",
    "dotenv": "tfk.env",
    "engines": {
      "node": "8.9.4"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.0",
    "micro-dev": "2.2.2",
    "nsp": "3.2.1",
    "nyc": "11.7.1",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "marked": "0.3.19",
    "micro": "9.3.0"
  }
}
