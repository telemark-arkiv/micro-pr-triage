{
  "name": "micro-pr-triage",
  "description": "Microservice for routing requests",
  "version": "1.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm -e GITHUB_USER=@micro-pr-github-user -e GITHUB_TOKEN=@micro-pr-github-token && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "github"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-pr-triage.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-pr-triage/issues"
  },
  "now": {
    "alias": [
      "github.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "coveralls": "2.12.0",
    "nsp": "2.6.3",
    "nyc": "10.2.0",
    "standard": "9.0.2",
    "test-listen": "1.0.1"
  },
  "dependencies": {
    "axios": "0.15.3",
    "marked": "0.3.6",
    "micro": "7.2.2",
    "wreck": "12.0.0"
  }
}
